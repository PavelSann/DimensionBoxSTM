echo Executing GDB with gdbinit to connect to OpenOCD.\n
# Start to OpenOCD
target extended-remote | C:/msys64/mingw64/bin/openocd.exe -c "gdb_port pipe;" -f st_board/nucleo_f207zg.cfg
# configure gdb
set remote hardware-breakpoint-limit 6
set remote hardware-watchpoint-limit 4
define hook-step
mon cortex_m maskisr on
end
define hookpost-step
mon cortex_m maskisr off
end
# Reset the target and call its init script
monitor reset init
# load 
file dist/Debug/GNUToolsARMEmbedded-Windows/stm32l1
load
#set breakpoints
break main
#run
continue
